    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@react-page/editor": "^2.4.0",
    "@react-page/plugins-slate": "^2.4.0",
    "@tiptap/core": "^2.0.0-beta.99",
    "@tiptap/react": "^2.0.0-beta.60",
    "@tiptap/starter-kit": "^2.0.0-beta.97",
